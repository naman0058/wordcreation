<!--blog start-->

<section>
  <div class="container">
    <div class="row">
        <% for(i=0;i<result.length;i++){ %>
      <div class="col-lg-4 col-md-6">
        <div class="post">
          <div class="post-image">
            <img class="img-fluid h-100 w-100" src="images/<%=result[i].image%>" alt="">
          </div>
          <div class="post-desc">
            <!-- <div class="post-date">23 <span>November 2018</span>
            </div> -->
            <div class="post-title">
              <h5><a href="/blogs/<%=result[i].seo_name%>"><%=result[i].name%></a></h5>
            </div>
            <p><%=result[i].description%></p>
         
          </div>
        </div>
      </div>
      <% } %>
    </div>
  </div>
</section>

<!--blog end-->



</div>

<!--body content end--> 

